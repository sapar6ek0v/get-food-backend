generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  ADMIN
  SUPER_ADMIN
  USER
}

model User {
  id       Int    @default(autoincrement()) @id
  email    String @unique
  name     String?
  password String 
  role     Role   @default(USER)
}

